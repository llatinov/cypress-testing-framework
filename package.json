{
  "name": "cypress-testing-framework",
  "version": "1.0.0",
  "description": "Complete end to end testing framework based on Cypress",
  "main": "index.js",
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-scripts build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "cypress:eslint": "eslint ./cypress",
    "cypress:examples:open": "cypress open --config integrationFolder=./cypress/examples",
    "cypress:examples:run": "cypress run --config integrationFolder=./cypress/examples,video=false",
    "cypress:open": "cypress open --config integrationFolder=./cypress/tests",
    "cypress:run": "cypress run --config integrationFolder=./cypress/tests,video=false",
    "cypress:retry": "node cypress/support/npm/cypress_retry_failed.js",
    "cypress:report": "node cypress/support/npm/cypress_merge_results.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/llatinov/cypress-testing-framework.git"
  },
  "keywords": [
    "Cypress",
    "testing",
    "framework"
  ],
  "author": "Lyudmil Latinov",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/llatinov/cypress-testing-framework/issues"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bootstrap": "^4.3.1",
    "eslint": "^5.16.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router": "^3.0.0",
    "react-scripts": "3.0.1",
    "toastr": "^2.1.4"
  },
  "devDependencies": {
    "cypress": "^3.3.1",
    "istanbul-instrumenter-loader": "^3.0.1",
    "junit-report-merger": "^0.0.6",
    "react-app-rewired": "^2.1.3",
    "xml-js": "^1.6.11"
  }
}
